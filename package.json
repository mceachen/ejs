{
  "name": "@mceachen/ejs",
  "description": "Embedded JavaScript templates",
  "keywords": [
    "template",
    "engine",
    "ejs"
  ],
  "version": "4.0.0",
  "author": "Matthew Eernisse <mde@fleegix.org> (http://fleegix.org)",
  "license": "Apache-2.0",
  "main": "./lib/ejs.js",
  "jsdelivr": "ejs.min.js",
  "unpkg": "ejs.min.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:mceachen/ejs.git"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "eslint": "^8.13.0",
    "git-directory-deploy": "^1.5.1",
    "jake": "^10.8.5",
    "jsdoc": "^3.6.10",
    "lru-cache": "^7.8.1",
    "mocha": "^9.2.2",
    "release-it": "^14.14.2",
    "uglify-js": "^3.15.4"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "jake": "jake -f jakefile.js",
    "test": "mocha",
    "release": "release-it"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "npm install",
        "npm run jake build",
        "npm run test",
        "npm run jake docPublish"
      ]
    },
    "github": {
      "release": true
    }
  }
}
